{
  "name": "Chrome Swift Open",
  "version": "0.0.1",
  "options_page": "html/options.html",
  "browser_action":
  {
    "default_icon": {
        "19": "images/icon.png",
     	"38": "images/icon.png"
     },
    "default_title": "Chrome Swift Open"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": ["tabs","storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
